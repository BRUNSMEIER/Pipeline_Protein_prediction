echo "Running script to compile the CMBI version of DSSP, please wait..."
cc -Wall -Wextra -Wuninitialized -c -O AccSurf.c
cc -Wall -Wextra -Wuninitialized -c -O CalcAccSurf.c
cc -Wall -Wextra -Wuninitialized -c -O Contacts.c
cc -Wall -Wextra -Wuninitialized -c -O Date.c
cc -Wall -Wextra -Wuninitialized -c -O DsspCMBI.c
cc -Wall -Wextra -Wuninitialized -c -O Vector.c
cc -Wall -Wextra -Wuninitialized -c -O p2clib.c
cc -o dsspcmbitest DsspCMBI.o p2clib.o Date.o Vector.o AccSurf.o CalcAccSurf.o Contacts.o -lm
echo "Type dsspcmbi PDBSourcefile DSSPDestinationfile to run the program..."
